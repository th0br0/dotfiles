! vim: ft=xdefaults

Xft.lcdfilter:  lcddefault
Xft.antialias:  true
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint: 0
Xft.dpi: 180
Xft.rgba: rgb

! -------------------------
! URXVT
! -------------------------

! Look & feel
URxvt.transparent: false
URxvt.depth: 32
! URxvt.fading: 60
URxvt*termName: rxvt-unicode-256color
URxvt*loginShell: true
URxvt*scrollBar: false
URxvt*secondaryScroll: true
URxvt*saveLines: 65535
URxvt*cursorUnderline: false
URxvt*urgentOnBell: true
URxvt*cursorBlink: false

! Extensions                                                                                                                   
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher,searchable-scrollback
URxvt*urlLauncher: google-chrome
URxvt*matcher.button: 1
URxvt.intensityStyles: false
! Fonts
URxvt.font:\
    xft:PragmataPro for Powerline:pixelsize=20,\
    xft:Monospace
URxvt.boldFont:\
    xft:PragmataPro for Powerline:pixelsize=20:style=Bold,\
    xft:Monospace
URxvt.italicFont:\
    xft:PragmataPro for Powerline:pixelsize=20:style=Italic,\
    xft:Monospace

URxvt.boldItalicFont:\
    xft:PragmataPro for Powerline:pixelsize=20:style=Bold Italic,\
    xft:Monospace

! Fix font width
URxvt.letterSpace: -1

urxvt*scrollBar:        false
urxvt*cursorBlink:      true
urxvt*cursorColor:      #c1c9c9


